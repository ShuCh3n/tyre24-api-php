{
    "name": "ediasoft/tyre24-api-php",
    "description": "Tyre24 API client library for PHP.",
    "license": "MIT",
    "version": "1.0.0",
    "type": "library",
    "require": {
        "php": ">=7.4|^8.0",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-fileinfo": "*",
        "monolog/monolog": "^2.2",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "composer/ca-bundle": "^1.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "symfony/var-dumper": "^5.4.9",
        "phpunit/phpunit": "^9.5",
        "vlucas/phpdotenv": "^5.4",
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "authors": [{
        "name": "eDiasoft",
        "email": "support@ediasoft.com",
        "homepage": "https://ediasoft.com",
        "role": "Developer"
    }],
    "autoload": {
        "psr-4": {
            "eDiasoft\\Tyre24": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "eDiasoftTests\\": "test/"
        }
    }
}
